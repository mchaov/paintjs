    "fork-ts-checker-webpack-plugin": "0.4.7",
    "fs-extra": "7.0.0",
    "image-webpack-loader": "4.3.1",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "mini-css-extract-plugin": "0.4.1",
    "cache-loader": "1.2.2",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "raw-loader": "0.5.1",
    "babel-preset-env": "1.7.0",
