{
  "name": "paintjs",
  "version": "0.0.1",
  "description": "One of my favourite university courses. Make MSPaint like program. This time using just JS :)",
  "scripts": {
    "build": "webpack --config ./.configs/webpack.release.js",
    "build-external": "webpack --config ./.configs/webpack.release.external.js",
    "build:release": "npm run test:coverage && npm run build",
    "build-w": "webpack --config ./.configs/webpack.release.js -w",
    "dev": "start http-server ./ -p 9001 && npm run build-w",
    "test": "jest --bail --config ./.configs/jest.config.js",
    "test:coverage": "jest --coverage --bail --config ./.configs/jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mchaov/paintjs.git"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.11",
    "@types/jest": "23.1.2",
    "@types/jsdom": "11.0.6",
    "@types/jsonwebtoken": "7.2.7",
    "@types/node": "10.3.6",
    "@types/react": "16.4.2",
    "@types/react-dom": "16.0.6",
    "@types/webpack": "4.4.3",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-adapter-utils": "1.3.0",
    "enzyme-to-json": "3.3.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fork-ts-checker-webpack-plugin": "0.4.2",
    "fs-extra": "6.0.1",
    "image-webpack-loader": "4.3.1",
    "jest": "23.2.0",
    "jest-cli": "23.2.0",
    "less": "3.0.4",
    "less-loader": "4.1.0",
    "mini-css-extract-plugin": "0.4.0",
    "cache-loader": "1.2.2",
    "compression-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "0.28.11",
    "promise-polyfill": "8.0.0",
    "raf": "3.4.0",
